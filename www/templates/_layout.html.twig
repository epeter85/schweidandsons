<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <title>{% block title %}Schweid &amp; Sons{% endblock %}</title>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta name="description" content="Our passion is carried on with a single-minded dedication to producing the best tasting, highest quality burger" />
    
    {% block keywords %}
    {% endblock %}
    
    {% block google %}
    {% endblock %}

    {% block twitter %}
    {% endblock %}
    
    {% block facebook %}
    {% endblock %}
        
    {% block stylesheets %}
    <link href="{{ siteUrl('css/style.min.css') }}" rel="stylesheet">
    {% endblock %}
    
    <script type="text/javascript">
      (function(d,s,i,r) {
        if (d.getElementById(i)){return;}
        var n=d.createElement(s),e=d.getElementsByTagName(s)[0];
        n.id=i;n.src='//js.hs-analytics.net/analytics/'+(Math.ceil(new Date()/r)*r)+'/438764.js';
        e.parentNode.insertBefore(n, e);
      })(document,"script","hs-analytics",300000);
    </script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-51940808-2', 'auto', {'allowLinker': true});
      
      // Load the plugin.
      ga('require', 'linker');
      // Define which domains to autoLink.
      ga('linker:autoLink', ['schweidandsons.com', 'info.schweidandsons.com', 'foodservice.schweidandsons.com']);

      ga('send', 'pageview');
    </script>
    
</head>

<body>
  
   <a href="https://plus.google.com/105955467379817747607" rel="publisher"></a>
   <div class="off-canvas-wrap" data-offcanvas="">
       <div class="inner-wrap">
             {% include '_nav.html.twig' %}
        </div>
    </div> 
           
    <section class="left-small">
      
            <a class="left-off-canvas-toggle"><img alt="" src="{{ siteUrl('images/burger.png') }}"></a>
 
    </section>
          
    <div id="video-container">
            <video id="video-player" autoplay="" loop="" poster="">
               <source src="" type="video/mp4">
                <source src="" type="video/webm">
                <source src="" type="video/ogg">
            </video>
          </div>
          
            <div class="curtain"></div>
          <div class= "main-content">
          {% block content %}{% endblock %}
          </div>

           <div class="small-text-right home-logo">
                <a href="/"><img src="{{ siteUrl('images/logo.png') }}" alt="Schweid & Sons logo"></a>
            </div>
            
                <div class="arrow-navigation">
      <a href="{{ siteUrl(product.previous.path) }}" class="arrow-back">
        <img src="{{ siteUrl('/images/arrow-back.png') }}" alt="previous product">
      </a>

      <a href="{{ siteUrl(product.next.path) }}" class="arrow-forward">
        <img src="{{ siteUrl('/images/arrow-forward.png') }}" alt="next product">
      </a>
    </div>

    {% block javascripts %} 
    <script src="http://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script> 
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <!--<script src="{{ siteUrl('js/local/modernizr.custom.52664.js') }}"></script> 
    <script src="{{ siteUrl('js/local/jquery-2.1.1.min.js') }}"></script>-->
    <script src="{{ siteUrl('js/foundation.min.js') }}"></script>
    <script src="{{ siteUrl('js/PxLoader.js') }}"></script>
    <script src="{{ siteUrl('js/PxLoaderImage.js') }}"></script>
    <script src="{{ siteUrl('js/script.min.js') }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
    <script>$(document).foundation();</script>
    {% endblock %}
</body>
</html>